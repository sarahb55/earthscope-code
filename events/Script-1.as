package earthscope.events{		import flash.events.Event;		public  class GameEvent extends Event{				private var _message:String;				public function setMessage(message:String):void{			_message = message;		}				public function getMessage():String{						return _message;		}				public function GameEvent(type:String, bubbles:Boolean= true, cancelable:Boolean = false){						super(type, bubbles, cancelable); 		}		}				 