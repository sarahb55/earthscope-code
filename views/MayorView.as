package earthscope.views{		import flash.display.Sprite;	import flash.events.Event;	import flash.display.MovieClip;	import flash.media.Sound;	import flash.net.URLRequest;		import earthscope.models.IGameData;	import earthscope.models.MayorData;	import earthscope.events.GameEvent;	import earthscope.utils.DisplayGraphicsFiles;				public class MayorView extends Sprite{								private var _mayorData:MayorData;		private var _hair:DisplayGraphicsFiles;		private var _nose:DisplayGraphicsFiles;		private var _body:DisplayGraphicsFiles;		private var _clothes:DisplayGraphicsFiles;		private var _scale:Number = 1.0;		private var _x :int= 0;		private var _y:int = 0;				//private var _puff:MovieClip;		//private var _volcanoData:VolcanoData;				public function MayorView(x:int, y:int, data:MayorData, scale:Number = 1.0):void{						_mayorData = data;						if(_mayorData == null){				return;			}						_x = x;			_y = y;			loadAssets();		}				public function set mayor(mayor:MayorData){						_mayorData = mayor;						trace("Returning Mayor...");						trace(_mayorData.hair + " " +  _mayorData.clothes + " "  + _mayorData.gender);		}				public function loadAssets():void{									_body = new DisplayGraphicsFiles();			trace(_mayorData.bodyFile);			_body.loadFile(_mayorData.bodyFile);			_hair = new DisplayGraphicsFiles();			_hair.loadFile(_mayorData.hairFile);			_clothes = new DisplayGraphicsFiles();			_clothes.loadFile(_mayorData.clothesFile);									addChild(_body);			addChild(_clothes);			addChild(_hair);						this.x = _x;			this.y = _y;					}				public function updateDisplay():void{						if(_body.parent == this){				removeChild(_body);			}			if(_clothes.parent == this){				removeChild(_clothes);			}			if(_hair.parent == this){				removeChild(_hair);			}						trace("Returning Mayor...");						trace(_mayorData.hair + " " +  _mayorData.clothes + " "  + _mayorData.gender);						loadAssets();		}							}				}